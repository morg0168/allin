@font-face {
  font-family: 'MaisonNeue-Bold';
  src: url('{{ 'MaisonNeue-Bold.eot?#iefix' | asset_url }}') format('embedded-opentype'),  url('{{ 'MaisonNeue-Bold.otf' | asset_url }}')  format('opentype'),
	     url('{{ 'MaisonNeue-Bold.woff' | asset_url }}') format('woff'), url('{{ 'MaisonNeue-Bold.ttf' | asset_url }}')  format('truetype'), url('{{ 'MaisonNeue-Bold.svg#MaisonNeue-Bold' | asset_url }}') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'MaisonNeue-Medium';
  src: url({{'MaisonNeue-Medium.eot?#iefix' | asset_url }}) format('embedded-opentype'),  url({{'MaisonNeue-Medium.otf' | asset_url }})  format('opentype'),
	     url({{'MaisonNeue-Medium.woff' | asset_url }}) format('woff'), url({{'MaisonNeue-Medium.ttf' | asset_url }})  format('truetype'), url({{'MaisonNeue-Medium.svg#MaisonNeue-Medium' | asset_url }}) format('svg');
  font-weight: normal;
  font-style: normal;
}

/* General */

body {
  background: white;
  text-rendering: optimizeLegibility !important;
  -webkit-font-smoothing: antialiased !important;
}

#closing-template {
  font-family: 'MaisonNeue-Medium' !important;
  font-size: 11px;
  background: white;
}

#footer {
  font-family: 'MaisonNeue-Medium' !important;
}

ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

p {
  margin: 0;
  padding: 0;
}
.container {
  padding: 0;
}
.row {
  margin: 0;
}

li a {
	color: #f60300;
    text-decoration: none;
  	cursor: pointer;
}

li a:focus, li a:hover {
    color: #f60300;
    text-decoration: none;
}

.preview-capsule a {
	color: #f60300;
    text-decoration: none;
  	cursor: pointer;
}

#footer a:focus, #footer a:hover {
    color: #f60300;
    text-decoration: none;
}

#footer a {
	color: #f60300;
    text-decoration: none;
  	cursor: pointer;
}

.preview-capsule a:focus, .preview-capsule a:hover {
    color: #f60300;
    text-decoration: none;
}

.no-gutters {
  margin-right: 0;
  margin-left: 0;
}

.col,[class*="col-"] {
    padding-right: 0;
    padding-left: 0;
  }
/* Header */

#header {
  color: #f60300;
  margin-top: 50px;
}

/* Content */

.img-holder img {
    width: 100%;
    max-width: 385px;
}

.logo-holder img {
    width: 75%;
    position: relative;
    bottom: -75px;
    display: block;
    margin: 0 auto;
    float: left;
}

.preview-capsule {
  display: flex;
  color: #f60300;
  position: relative;
}

.preview-capsule img {
    width: 6px;
    height: 55px;
    position: absolute;
    left: 400px;
    top: 5px;
}

#stockists {
  color: #f60300;
  margin-bottom: 30px;
}
#dealers-holder {
  display: flex;
  color: #f60300;
  margin-bottom: 150px;
}
#dealer-list-1 {
  text-transform: uppercase;
}
.dealers {
  width: 50%;
}
.transparent {
  visibility: hidden;
}
/* footer */
.full {
  background: white;
  margin-top: 50px;
}

#footer-holder {
  color: #f60300;
  font-size: 11px;
}
#footer {
  margin: 20px 0;
}
.float-left {
  float: right;
}
.float-right {
  float: right;
  margin-right: 100px;
}

.page-capsule .float-right {
    float: right;
    margin-right: 100px;
}

.page-capsule .float-left {
    float: right;
}

#second-haft-mobile {
  display: none;
}
#first-haft-mobile {
  display: none;
}

/* -- Slider --*/

.glyphicon-chevron-left:before {
  background: url({{ 'arrow-left.svg' | asset_url }}) no-repeat center 0px;
  background-size: 30px;
  color: transparent;
}

.glyphicon-chevron-right:before {
  background: url({{ 'arrow-right.svg' | asset_url }}) no-repeat center 0px;
  background-size: 30px;
  color: transparent;
}

.carousel {
    position: relative;
    width: 476px;
    margin: 0 auto;
}

.carousel-control {
  opacity: 1;
  text-shadow: none;
}

.carousel-control.right {
  background-image: none;
  right: -30px;
}

.carousel-control.left {
  background-image: none;
  left: -30px;
}

.page-capsule #logo {
  width: 80px;
  margin-top: 80px;
}

.page-capsule .container {
  margin: 0 auto;
}

.carousel-inner {
    position: relative;
    width: 80%;
    overflow: hidden;
    margin: 0 auto;
}

/* Rest of Capsule page */

.page-capsule h3 {
  font-family: 'MaisonNeue-Medium' !important;
  font-size: 11px;
  color: #ED3230;
  margin-bottom: 60px
}

.page-capsule .right-text p {
  font-family: 'MaisonNeue-Medium' !important;
  margin-bottom: 20px;
  font-size: 11px;
  color: #ED3230;
}

.page-capsule .right-text {
    position: relative;
    top: -44px;
  	width: 300px;
}

.page-capsule .back a {
    display: flex;
    font-family: 'MaisonNeue-Medium' !important;
    font-size: 11px;
    color: #ED3230;
    position: fixed;
    bottom: 27%;
}

_::-webkit-full-page-media, _:future, :root .back-text {

    position: relative;
    top: -50px;


}

_::-webkit-full-page-media, _:future, :root #closing-template .skip img {

    top: 3px;


}

     #intro #mobilevid {
     	display: none;
     }

.carousel-indicators {
  display: none;
}

.page-capsule .back img {
  width: 25px;
  position: relative;
  top: -50px;
}

.page-capsule .slide-number {
  position: relative;
  left: 69px;
  top: -25px;
  color: #ED3230;
  font-family: 'MaisonNeue-Medium' !important;
  font-size: 11px;
}


#closing-template .skip a {
    display: flex;
    position: fixed;
    font-family: 'MaisonNeue-Medium' !important;
    font-size: 11px;
    color: #ED3230;
    top: 20px;
    right: 20px;
    text-decoration: none;
  	z-index: 99999;
}

#closing-template .skip-mobile a {
    display: flex;
    font-family: 'MaisonNeue-Medium' !important;
    font-size: 11px;
    color: #ED3230;
    text-decoration: none;
}

#intro #footer {
  display: none;
}

#intro #video {
  display: flex;
  justify-content: center;
  align-items: center;
  background: black;
}

#closing-template .skip img {
    width: 25px;
    position: relative;
    top: -41px;
    right: -5px;
}

#intro video {
    width: 87vw;
    margin-top: -7px;
}

#intro #footer ul {
  display: flex;
  justify-content: space-around;
}

#intro {
    width: 100% !important;
    height: 100vh;
    background: black;
}

#intro #footer li {
    width: 19%;
}

#intro .closer-look-inner {
  display: flex;
  font-family: 'MaisonNeue-Medium' !important;
  font-size: 11px;
  color: #ED3230;
  width: 100%;
  position: relative;
}

#intro .closer-look-inner p:first-child {
  margin-bottom: 10px;
}

#intro .closer-look img {
    width: 25px;
    height: 30px;
    position: absolute;
    top: -9px;
    right: 0px;
}

#intro .closer-look {
  margin-top: 15px;
}

#logo-all-in {
  width: 278px;
  margin-bottom: 60px;
}

.axel {
  width: 90px
}
.template-index p {
  font-size: 11px;
  color: #f60300;
  font-family: 'MaisonNeue-Medium' !important;
  text-rendering: optimizeLegibility !important;
  -webkit-font-smoothing: antialiased !important;
  letter-spacing: -0.5px;
}

.template-index #capsule-content p {
  margin-bottom: 30px;
}

.template-index #capsule-content #last {
  margin-bottom: 70px;
}

.template-index .img-holder {
  float: right;
}

.template-index .skip-content {
  text-align: center;
}

#second-haft-mobile #capsule-content .logo-holder {
  margin-bottom: 60px;
}

.template-index #first-haft-mobile .img-holder {
  float: none;
  text-align: center;
  margin-bottom: 60px;
}

.page-capsule #footer-holder {
  color: #f60300;
  font-size: 11px;
  position: relative;
  left: 40px;
  bottom: -80px;
}







/* Responsive */

/* Large Devices, Wide Screens */
   @media only screen and (min-width : 1800px) {
	#closing-template {
    font-family: 'MaisonNeue-Medium' !important;
    font-size: 11px;
    background: white;
    display: flex;
    flex-direction: column;
    justify-content: center;
	}

  }

@media only screen and (min-width : 1500px) {

	.page-capsule #footer-holder {
    color: #f60300;
    font-size: 11px;
    position: relative;
    left: 40px;
    bottom: -80px;
	}
}

  @media only screen and (max-width : 1199px) {
    .page-capsule .slide-number {
    left: 49px;
  }
    #intro video {
    width: 96vw;
	}
  }

  @media only screen and (max-width : 1199px) {
    #intro #video {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100vh;
    }
  }

   /* Medium Devices, Desktops */
