    margin: 0 auto;
}
.page-capsule .right-text {
    position: relative;
    top: 0;
    width: 560px;
    margin: 0 auto;
    margin-bottom: 60px;
}

   }

   /* Small Devices, Tablets */
   @media only screen and (max-width : 767px) {
     #intro #desktopvid {
     	display: none;
     }
     #intro #mobilevid {
     	display: block;
     }
     #second-haft-desktop {
       display: none;
     }
     #header {
     	display: none;
     }
     #first-haft-desktop img {
     	display: none;
     }
     #second-haft-mobile {
       display: block;
       margin-bottom: 180px;
     }
     #first-haft-mobile {
       display: block;
     }
     #closing-template {
       font-size: 11px;
       margin: 0 60px;
     }
     #footer-holder {
       font-size: 11px;
       margin: 0 60px;
     }
     #closing-template .skip img {
    	width: 25px;
    	position: relative;
    	top: -45px;
    	right: -5px;
	}

	.preview-capsule {
	    margin-bottom: 40px;
	    margin-top: 15px;
	    margin-left: 60px;
      	font-size: 11px;
      	font-family: 'MaisonNeue-Medium' !important;
	}
    .preview-capsule img {
    	display: block !important;
    	right: -60px;
    	width: 15px;
    	height: 80px;
      	left: initial;
    }
    .logo-holder img {
      width: 85%;
      display: block;
      margin: 0 auto;
      position: initial;
      float: initial;
    }
    #dealer-list-1 {
      width: 50%;
    }
    #dealer-list-2 {
      width: 50%;
    }
    #dealer-list-3 {
      text-align: right;
      width: 25%;
    }
    .float-right {
      float: left;
      margin-top: 35px;
    }
    .float-left {
      line-height: 0.8;
    }
    #footer {
      margin: 60px 0;
    }
     #stockists {
    position: initial;
    margin-left: 0px;
    margin-top: 30px;
	}
    .full {
      margin-top: 0px;
  }
  .page-capsule #footer-holder {
    font-size: 11px;
    margin: 0 auto;
  }
  .page-capsule .float-left {
  margin-right: 0px;
  margin-left: 80px;
  float: left;
}
.page-capsule .float-right {
    margin-right: 0px;
    float: left;
    margin-left: 80px;
}
.carousel-control {
    font-size: 30px;
}
.carousel {
    position: relative;
    width: 400px;
    margin: 0 auto;
}
.page-capsule .slide-number {
    left: 41px;
}
.page-capsule .right-text {
    position: relative;
    top: 0;
    width: 320px;
    margin: 0 auto;
    margin-bottom: 60px;
}
}

   /* Extra Small Devices, Phones */
   @media only screen and (max-width : 612px) {
     #closing-template {
       font-size: 10px;
       margin: 0 25px;
      }
     .preview-capsule {
    	margin-bottom: 40px;
    	margin-top: 15px;
    	margin-left: 25px;
    	font-size: 10px;
    	font-family: 'MaisonNeue-Medium' !important;
	  }
     .preview-capsule img {
          display: block !important;
          right: -76px;
          width: 9px;
          height: 40px;
          left: initial;
      }
      #header {
        margin-top: 25px;
      }
      #second-haft-mobile {
        margin-bottom: 120px;
      }
      #dealers-holder {
        margin-bottom: 90px;
      }
      #footer-holder {
        font-size: 10px;
        margin: 0 25px;
      }
   }

@media only screen and (max-width : 500px) {
  .carousel {
     position: relative;
     width: 300px;
     margin: 0 auto;
  }
  .carousel-control.left {
     left: -20px;
  }
  .carousel-control.right {
    right: -20px;
  }
  .page-capsule #logo {
    margin-left: 35px;
  }
  .page-capsule .float-left {
    margin-left: 35px;
  }
  .page-capsule .float-right {
    margin-left: 35px;
  }
  .page-capsule .right-text {
    position: relative;
    top: 0;
    width: 240px;
    margin: 0 auto;
    margin-bottom: 60px;
}
}
